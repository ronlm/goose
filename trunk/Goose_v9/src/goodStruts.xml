<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<!-- 这个Struts配置文件用于物资模块 -->	
	<package name="good" extends="struts-default" namespace="/pages/good">	
		<!-- 使用通配符 对农户，养殖场配置，页面名第一个单词用小写 开始-->
		<action name="*Action" class="{1}Action" >
			<result name="edit">/pages/good/{1}Edit.jsp</result>
			<result name="error">/error/WebError.jsp</result>
			<result name="list">/pages/good/{1}List.jsp</result>
		</action>
		<!-- 使用通配符 对物资模块配置 结束-->
	</package>
</struts>
	